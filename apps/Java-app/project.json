{
  "name": "java-app",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/java-app",
        "commands": [
          {
            "command": "/usr/bin/mvn",
            "args": ["clean", "package"]
          }
        ]
      }
    },
    "run": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/java-app",
        "commands": [
          {
            "command": "/usr/bin/mvn",
            "args": ["spring-boot:run"]
          }
        ]
      }
    }
  }
}

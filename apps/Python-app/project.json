{
  "name": "Python-app",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/Python-app",
        "command": "python -m compileall ."
      }
    },
    "run": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/Python-app",
        "command": "python main.py"
      }
    }
  }
}
